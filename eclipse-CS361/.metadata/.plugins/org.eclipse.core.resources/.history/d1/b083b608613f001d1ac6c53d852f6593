package Lessons;

public class FactFib {
	
	public static int fact (int n) {
		if(n==0) {
			return 0;
		}
		else {
			return n * fact(n-1);
		}
	}
	
	public static int fib(int n) {
		if(n==0||n==1) {
			return 1;
		}
		else {
			return fib(n-1)+fib(n-2);
		}
	}
	
	
	public static void main (String[] args) {
		System.out.println("fib= " + fib(5)+ ", fact= " + fact(5));
	}
}
